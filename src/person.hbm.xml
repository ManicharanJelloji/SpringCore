<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.spring.hibernate.Person" table="PERSON1">
		<id name="id" column="PERSON_ID" type="long">
			<!--<generator class="native"></generator>-->
		</id>
		<property name="firstName" column="FIRST_NAME" type="string"></property>
		<property name="lastName" column="LAST_NAME" type="string"></property>
		
	</class>
</hibernate-mapping>